<resources>
    <declare-styleable name="CircledNetworkImageView">
        <attr name="cniv_borderColor" format="color" />
        <attr name="cniv_borderWidth" format="dimension" />
        <attr name="cniv_placeholder" format="reference" />
        <attr name="cniv_asCircle" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
