<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item android:left="14dp" android:src="@drawable/ic_gender_edit_text">
                <bitmap android:gravity="center_vertical|left" android:src="@drawable/ic_playlist_add_black_24dp" />
            </item>
            <item>
                <bitmap android:gravity="bottom|right" android:src="@drawable/spinner_ab_default_holo_dark_am" />
            </item>
            <item android:gravity="bottom" android:left="-10dp" android:right="-10dp" android:top="-10dp">
                <shape>
                    <stroke android:width="1dp" android:color="@color/divider" />
                    <solid android:color="@android:color/transparent" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>